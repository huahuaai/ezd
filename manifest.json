{
  "manifest_version": 3,
  "name": "EzDrop",
  "version": "1.0.0",
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "host_permissions": [
    "https://mp.weixin.qq.com/*"
  ],
  "permissions": [
    "storage",
    "tabs",
    "sidePanel",
    "scripting",
    "alarms",
    "contextMenus",
    "downloads",
    "scripting",
    "clipboardRead",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "src/assets/icon64.png",
      "32": "src/assets/icon64.png",
      "48": "src/assets/icon64.png",
      "128": "src/assets/icon64.png"
    }
  },
  "icons": {
    "16": "src/assets/icon64.png",
    "32": "src/assets/icon64.png",
    "48": "src/assets/icon64.png",
    "128": "src/assets/icon64.png"
  },
  "side_panel": {
    "default_path": "src/sidepanel/index.html"
  },
  "options_page": "src/options/index.html",
  "content_scripts": [
    {
      "js": [
        "assets/material_script.js-loader-CKfDjc0E.js"
      ],
      "matches": [
        "https://mp.weixin.qq.com/cgi-bin/filepage*",
        "https://mp.weixin.qq.com/cgi-bin/appmsg*count=10*action=list_video*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "assets/edit_script.js-loader-Ig35XJAg.js"
      ],
      "matches": [
        "https://mp.weixin.qq.com/cgi-bin/appmsg*/appmsg_edit*"
      ],
      "css": [
        "src/edit.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://mp.weixin.qq.com/*"
      ],
      "resources": [
        "src/edit.css",
        "src/assets/**"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://mp.weixin.qq.com/*",
        "https://mp.weixin.qq.com/*"
      ],
      "resources": [
        "assets/material_script.js-vMQofgBd.js"
      ],
      "use_dynamic_url": true
    },
    {
      "matches": [
        "https://mp.weixin.qq.com/*"
      ],
      "resources": [
        "assets/edit_script.js-CZePGel6.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
